    <span class="o">};</span>

    <span class="c">/**</span>
<span class="c">     * Return new array as result of call provided function of the elements of input array</span>
<span class="c">     * @method map</span>
<span class="c">     * @static</span>
<span class="c">     * @param fn {Function} function</span>
<span class="c">     * @param a {Array} input array</span>
<span class="c">     * @param c {Object} context to function evaluate</span>
<span class="c">     * @return {Array}</span>
<span class="c">     */</span>
    <span class="nx">A</span><span class="o">.</span><span class="nx">map</span> <span class="o">=</span> <span class="o">(</span><span class="nx">Native</span><span class="o">.</span><span class="nx">map</span><span class="o">)</span> <span class="o">?</span>
