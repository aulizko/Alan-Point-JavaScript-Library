    <span class="o">};</span>
<span class="o">},</span> <span class="s1">&#39;1.0.0&#39;</span><span class="o">,</span> <span class="o">[</span> <span class="o">{</span> <span class="nx">name</span> <span class="o">:</span> <span class="s1">&#39;lang&#39;</span><span class="o">,</span> <span class="nx">minversion</span> <span class="o">:</span> <span class="s1">&#39;0.0.3&#39;</span> <span class="o">}</span> <span class="o">]);</span>
