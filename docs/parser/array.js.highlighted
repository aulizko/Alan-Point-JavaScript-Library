<span class="c">     * @param fn {Function} function which will evaluate elements</span>
<span class="c">     * @param c {Object} Context of the function</span>
<span class="c">     * @return {Boolean} true if every elements match.</span>
<span class="c">     */</span>
    <span class="nx">A</span><span class="o">.</span><span class="nx">every</span> <span class="o">=</span> <span class="o">(</span><span class="nx">Native</span><span class="o">.</span><span class="nx">every</span><span class="o">)</span> <span class="o">?</span> 
        <span class="k">function</span> <span class="o">(</span><span class="nx">a</span><span class="o">,</span> <span class="nx">fn</span><span class="o">,</span> <span class="nx">c</span><span class="o">)</span> <span class="o">{</span>
            <span class="k">return</span> <span class="nx">a</span><span class="o">.</span><span class="nx">every</span><span class="o">(</span><span class="nx">fn</span><span class="o">,</span> <span class="nx">c</span> <span class="o">||</span> <span class="nx">AP</span><span class="o">.</span><span class="nx">config</span><span class="o">.</span><span class="nx">win</span><span class="o">);</span>
        <span class="o">}</span> <span class="o">:</span>
