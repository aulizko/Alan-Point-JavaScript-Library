<span class="c">     * @return {Array} cleaned array</span>
<span class="c">     */</span>
    <span class="nx">A</span><span class="o">.</span><span class="nx">clean</span> <span class="o">=</span> <span class="k">function</span> <span class="o">(</span><span class="nx">input</span><span class="o">)</span> <span class="o">{</span>
